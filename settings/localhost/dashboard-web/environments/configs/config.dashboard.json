{
  "environments": ["coeus", "prod"],
  "services": [
    {
      "name": "healthmonitor-api",
      "type": "api",
      "groups": ["tools"],
      "instances": {
        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/health" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/health" }]
      },
      "description": "Overall health of the environment, inclusive of all services"
    },
    {
      "name": "status-web",
      "type": "api",
      "groups": ["tools"],
      "instances": {
        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/status-web" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/status-web" }]
      },
      "description": "The status website you're most likely looking at right now."
    },
    {
      "name": "identityserver",
      "type": "api",
      "groups": ["auth"],
      "instances": {
        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/identityserver" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/identityserver" }]
      },
      "description": "Processes authorization requests for all applications"
    },
    {
      "name": "policyserver",
      "type": "api",
      "groups": ["auth"],
      "instances": {
        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/policyserver" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/policyserver" }]
