        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/shoppingcart-api" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/shoppingcart-api" }]
      },
      "description": "shoppingcart-api"
    },
    {
      "name": "shoppingcart-web",
      "type": "api",
      "groups": ["shoppingcart"],
      "instances": {
        "coeus": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/shoppingcart-web" }],
        "prod": [{ "key": "001", "health": "http://kehlstein:5005/api/v1/services/shoppingcart-web" }]
