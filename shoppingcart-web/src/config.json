{
    "service": {
        "name": "shoppingcart-web"
    },
    "catalogApi": {
        "url": "http://localhost:5001"
    },
    "shoppingCartApi": {
        "url": "http://localhost:5000"
    },
    "identity": {
        "authority": "https://localhost:5002",
        "clientId": "shoppingcart-web",
        "responseType": "id_token token",
        "scope": "openid profile shoppingcart-api catalog-api",
        "redirectUri": "http://localhost:4200/login-redirect",
        "silentRedirectUri": "http://localhost:4200/signin-oidc",
        "postLogoutRedirectUri": "http://localhost:4200/signout-oidc",
        "accessTokenExpiringNotificationTime": 60,
        "loadUserInfo": true,
        "automaticSilentRenew": true,
        "logLevel": 0    
    }
}
