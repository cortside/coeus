{
    "version":  "1.63",
    "service":  "identityserver",
    "repository":  {
                       "name":  "Acme.IdentityServer"
                   },
    "database":  {
                     "name":  "IdentityServer",
                     "dbContext":  "IdentityServerDbContext",
                     "dbContextProject":  "src/Acme.IdentityServer.WebApi",
                     "startupProject":  "src/Acme.IdentityServer.WebApi",
                     "triggers":  {
                                      "excludeTables":  [
                                                            "__EFMigrationsHistory",
                                                            "AuditLog",
                                                            "AuditLogTransaction",
                                                            "Outbox"
                                                        ]
                                  }
                 },
    "docker":  {
                   "buildImage":  "Acme.azurecr.io/dotnet-sdk:6.0-alpine",
                   "runtimeImage":  "Acme.azurecr.io/dotnet-runtime:6.0-alpine",
                   "image":  "Acme.azurecr.io/identityserver",
                   "executable":  "Acme.IdentityServer.WebApi"
               },
    "build":  {
                  "configuration":  "Debug",
                  "publishableProject":  "Acme.IdentityServer.WebApi",
                  "nugetfeed":  "https://api.nuget.org/v3/index.json"
              },
    "sonar":  {
                  "organization":  "Acme",
                  "key":  "Acme_identityserver",
                  "host":  "https://sonarcloud.io",
                  "token":  "",
                  "propertyPrefix":  "/d:"
              }
}
