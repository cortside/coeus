<div class="grants">
    <div class="row page-header">
        <div class="col-sm-10">
            <h1>
                Claims
            </h1>
        </div>
    </div>
    <div>
        @foreach (var claim in User.Claims.OrderBy(x => x.Type).ToList())
        {
            <div>
                @claim.Type = @claim.Value
            </div>
        }
    </div>
</div>
